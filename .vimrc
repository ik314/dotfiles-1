syntax on
set smartindent
set number
set tabstop=4 softtabstop=0 expandtab shiftwidth=4 smarttab
set cc=90
highlight ColorColumn ctermbg=235 guibg=#2c2d27
highlight LineNr ctermfg=grey
inoremap jj <Esc> 
map <F2> :bp <CR> 
map <F3> :bn <CR>
map <F7> :tabp <CR> 
map <F8> :tabn <CR>
set nolist wrap linebreak breakat&vim

"nmap <C-j> 5j5<C-e>
"nmap <C-k> 5k5<C-y> 
